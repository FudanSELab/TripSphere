# LLM Configuration
# Required: Your OpenAI API key
LLM__API_KEY= "your_api_key"

# Optional: Custom API base URL for OpenAI-compatible APIs
# Leave commented to use default OpenAI API
# LLM__BASE_URL=https://api.openai.com/v1
LLM__BASE_URL="your_api_base_url"

# Model configuration
LLM__MODEL="your_model_name"
LLM__TEMPERATURE="your_temperature"
LLM__MAX_TOKENS="your_max_tokens"

# gRPC Server Configuration
GRPC__PORT="your_port"

# Service Configuration
SERVICE__NAME="your_service_name"
SERVICE__NAMESPACE=tripsphere

# Nacos Configuration (for service discovery)
NACOS__SERVER_ADDRESS="your_nacos_server_address"
NACOS__NAMESPACE_ID="your_nacos_namespace_id"
NACOS__GROUP_NAME="your_nacos_group_name"
NACOS__ENABLED=false
